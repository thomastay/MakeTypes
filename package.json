    "synthesis"
  ],
  "author": "John Vilk <jvilk@cs.umass.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jvilk/MakeTypes/issues"
  },
  "homepage": "https://github.com/jvilk/MakeTypes#readme",
  "devDependencies": {
    "@types/node": "^15.12.2",
    "chalk": "^4.1.1",
    "esbuild": "^0.12.9",
    "prettier": "^2.3.1",
    "typescript": "^4"
  },
  "dependencies": {
    "yargs": "^17"
  }
}
